<% if (title) { %>
  <h1><%= title %></h1>
<% } else { 
  res.locals.title = "Account Management" 
} %>

<!-- Flash Messages -->
<%- messages() %>

<!-- Display Validation Errors -->
<% if (errors) { %>
  <ul class="notice">
    <% errors.array().forEach(error => { %>
      <li><%= error.msg %></li>
    <% }) %>
  </ul>
<% } %>

<p>You're logged in</p>